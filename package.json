{
  "name": "serverlog-to-browser",
  "version": "1.1.2",
  "exports": {
    "./server": {
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js"
    },
    "./client": {
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junhakjh/server-log.git"
  },
  "keywords": [
    "react",
    "nextjs",
    "front-end",
    "web",
    "server",
    "browser",
    "client",
    "log",
    "server-log"
  ],
  "author": "junhakjh <junhakjh@ajou.ac.kr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/junhakjh/server-log/issues"
  },
  "homepage": "https://github.com/junhakjh/server-log#readme",
  "description": "A real-time logging library for server-side and client-side logs, supporting log levels (log, dir, debug, info, warn, error) and development mode logging. It enables seamless integration with Next.js, embedding server logs in HTML and providing console output on the client side.",
